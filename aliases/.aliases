#reload dotfiles
alias rl="source ~/.zshrc"

#Basic Tasks
alias ..='cd ..'
alias l='ls -alh'
alias ll='ls -alth'
alias j='jobs'

#Customize
alias c='clear'
alias bug='cd ~/bin/dotfiles'

#Sublime Text
#ref -> http://blog.alexmaccaw.com/sublime-text
alias e='subl . &'

#NewDir
take(){
	mkdir $1
	cd $1
}
